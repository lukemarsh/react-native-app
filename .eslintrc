{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "jest": true,
    "node": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "semi": [
      2,
      "always"
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "consistent-this": [
      2,
      "_this"
    ],
    "curly": [
      2,
      "all"
    ],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": [
      2,
      {
        "afterColon": true,
        "beforeColon": false
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": true
      }
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-with": 2,
    "one-var": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "quotes": [
      2,
      "single"
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "keyword-spacing": [
      2
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": 2,
    "space-return-throw-case": 0,
    "space-unary-ops": 0,
    "strict": [
      2,
      "function"
    ],
    "wrap-iife": 2,
    "yoda": [
      2,
      "never"
    ]
  }
}
